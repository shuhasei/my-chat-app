  if (
    originalFile.type.startsWith("image") &amp;&amp;
    originalFile.type !=3D=3D "image/gif"
  ) {
    file =3D await processFileBeforeUpload(originalFile);
  }
  let type =3D "file";
  if (file.type.startsWith("image")) type =3D "image";
  else if (file.type.startsWith("video")) type =3D "video";
  else if (file.type.startsWith("audio")) type =3D "audio";

  if (file.size &gt; 1024 * 1024 || type =3D=3D=3D "video" || type =3D=3D=
=3D "file") {
    const objectUrl =3D URL.createObjectURL(file);
    callback(objectUrl, type, file);
  } else {
    const reader =3D new FileReader();
    reader.onload =3D (event) =3D&gt; callback(event.target.result, type, f=
ile);
    reader.readAsDataURL(file);
  }
};

const handleCompressedUpload =3D (e, callback) =3D&gt; {
