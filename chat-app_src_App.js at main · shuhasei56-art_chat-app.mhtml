const generateThumbnail =3D (file) =3D&gt; {
  return new Promise((resolve) =3D&gt; {
    if (!file) {
      resolve(null);
      return;
    }
    const MAX_SIZE =3D 320;
    if (file.type.startsWith("image")) {
      const reader =3D new FileReader();
      reader.onload =3D (e) =3D&gt; {
        const img =3D new Image();
        img.onload =3D () =3D&gt; {
          const canvas =3D document.createElement("canvas");
          let width =3D img.width;
          let height =3D img.height;
          if (width &gt; height) {
            if (width &gt; MAX_SIZE) {
              height *=3D MAX_SIZE / width;
              width =3D MAX_SIZE;
            }
          } else {
            if (height &gt; MAX_SIZE) {
              width *=3D MAX_SIZE / height;
              height =3D MAX_SIZE;
            }
          }
          canvas.width =3D width;
          canvas.height =3D height;
          const ctx =3D canvas.getContext("2d");
          ctx.drawImage(img, 0, 0, width, height);
          resolve(canvas.toDataURL("image/jpeg", 0.5));
        };
        img.onerror =3D () =3D&gt; resolve(null);
        img.src =3D e.target.result;
      };
      reader.onerror =3D () =3D&gt; resolve(null);
      reader.readAsDataURL(file);
    } else if (file.type.startsWith("video")) {
      const video =3D document.createElement("video");
      video.muted =3D true;
      video.playsInline =3D true;
      video.crossOrigin =3D "anonymous";
      video.preload =3D "metadata";
      const capture =3D () =3D&gt; {
        try {
          const canvas =3D document.createElement("canvas");
          let width =3D video.videoWidth;
          let height =3D video.videoHeight;
          if (!width || !height) {
            resolve(null);
            return;
          }
          if (width &gt; height) {
            if (width &gt; MAX_SIZE) {
              height *=3D MAX_SIZE / width;
              width =3D MAX_SIZE;
            }
          } else {
            if (height &gt; MAX_SIZE) {
              width *=3D MAX_SIZE / height;
              height =3D MAX_SIZE;
            }
          }
          canvas.width =3D width;
          canvas.height =3D height;
          const ctx =3D canvas.getContext("2d");
          ctx.drawImage(video, 0, 0, width, height);
          const dataUrl =3D canvas.toDataURL("image/jpeg", 0.5);
          video.src =3D "";
          video.load();
          resolve(dataUrl);
        } catch (e) {
          resolve(null);
        }
      };
      video.onloadeddata =3D () =3D&gt; {
        video.currentTime =3D 0.5;
      };
      video.onseeked =3D () =3D&gt; {
        capture();
      };
      video.onerror =3D () =3D&gt; resolve(null);
      setTimeout(() =3D&gt; resolve(null), 2000);
      try {
        video.src =3D URL.createObjectURL(file);
      } catch (e) {
        resolve(null);
      }
    } else {
      resolve(null);
    }
  });
};

