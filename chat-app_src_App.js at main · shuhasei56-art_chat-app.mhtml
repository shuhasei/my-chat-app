const PostItem =3D ({ post, user, allUsers, db, appId, profile }) =3D&gt; {
  const [commentText, setCommentText] =3D useState(""),
    [mediaSrc, setMediaSrc] =3D useState(post.media),
    [isLoadingMedia, setIsLoadingMedia] =3D useState(false);
  const u =3D allUsers.find((x) =3D&gt; x.uid =3D=3D=3D post.userId),
    isLiked =3D post.likes?.includes(user?.uid);
  useEffect(() =3D&gt; {
    if (post.hasChunks &amp;&amp; !mediaSrc) {
      setIsLoadingMedia(true);
      (async () =3D&gt; {
        let base64Data =3D "";
        if (post.chunkCount) {
          const chunkPromises =3D [];
          for (let i =3D 0; i &lt; post.chunkCount; i++)
            chunkPromises.push(
              getDoc(
                doc(
                  db,
                  "artifacts",
                  appId,
                  "public",
                  "data",
                  "posts",
                  post.id,
                  "chunks",
                  `${i}`
                )
              )
            );
          const chunkDocs =3D await Promise.all(chunkPromises);
          chunkDocs.forEach((d) =3D&gt; {
            if (d.exists()) base64Data +=3D d.data().data;
          });
        } else {
          const snap =3D await getDocs(
            query(
              collection(
                db,
                "artifacts",
                appId,
                "public",
                "data",
                "posts",
                post.id,
                "chunks"
              ),
              orderBy("index", "asc")
            )
          );
          snap.forEach((d) =3D&gt; (base64Data +=3D d.data().data));
        }
        if (base64Data) {
          try {
            const mimeType =3D
              post.mimeType ||
              (post.mediaType =3D=3D=3D "video" ? "video/mp4" : "image/jpeg=
");
            const byteCharacters =3D atob(base64Data);
            const byteNumbers =3D new Array(byteCharacters.length);
            for (let i =3D 0; i &lt; byteCharacters.length; i++)
              byteNumbers[i] =3D byteCharacters.charCodeAt(i);
            const blob =3D new Blob([new Uint8Array(byteNumbers)], {
              type: mimeType,
            });
            setMediaSrc(URL.createObjectURL(blob));
          } catch (e) {
