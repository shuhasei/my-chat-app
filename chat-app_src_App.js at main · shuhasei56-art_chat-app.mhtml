            loading=3D"lazy"
          /&gt;
          {isTodayBirthday(u?.birthday) &amp;&amp; (
            &lt;span className=3D"absolute -top-1 -right-1 text-xs"&gt;=F0=
=9F=8E=82&lt;/span&gt;
          )}
        &lt;/div&gt;
        &lt;div className=3D"font-bold text-sm"&gt;{u?.name}&lt;/div&gt;
      &lt;/div&gt;
      &lt;div className=3D"text-sm mb-3 whitespace-pre-wrap"&gt;{post.conte=
nt}&lt;/div&gt;
      {(mediaSrc || isLoadingMedia) &amp;&amp; (
        &lt;div className=3D"mb-3 bg-gray-50 rounded-2xl flex items-center =
justify-center min-h-[100px]"&gt;
          {isLoadingMedia ? (
