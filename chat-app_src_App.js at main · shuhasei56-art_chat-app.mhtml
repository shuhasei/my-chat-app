      {
        comments: arrayUnion({
          userId: user.uid,
          userName: profile.name,
          text: commentText,
          createdAt: new Date().toISOString(),
        }),
      }
    );
    setCommentText("");
  };
  return (
    &lt;div className=3D"bg-white p-4 mb-2 border-b"&gt;
      &lt;div className=3D"flex items-center gap-3 mb-3"&gt;
        &lt;div className=3D"relative"&gt;
          &lt;img
            key=3D{u?.avatar}
